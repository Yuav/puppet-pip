{
  "name": "onpuppet-pip",
  "version": "0.2.0",
  "author": "Jon Skarpeteig",
  "summary": "Custom python pip package provider that's less broken",
  "license": "Apache-2.0",
  "source": "https://github.com/onpuppet/puppet-pip.git",
  "project_page": "https://github.com/onpuppet/puppet-pip",
  "issues_url": "https://github.com/onpuppet/puppet-pip/issues",
  "dependencies": [
    { "name": "stahnma-epel", "version_requirement": ">= 1.0.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 3.0.0" }
  ],
  "tags": [
    "python",
    "pip",
    "provider"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}

